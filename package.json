{
  "name": "@w11k/ngx-present-demo",
  "version": "0.4.5",
  "description": "Open Source, Angular Based Presentation Tool",
  "author": "W11K GmbH, Germany",
  "contributors": [
    "Philipp Burgmer"
  ],
  "homepage": "https://github.com/w11k/ngx-present",
  "repository": {
    "type": "git",
    "url": "https://github.com/w11k/ngx-present.git"
  },
  "keywords": [],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "rimraf ./dist && npm run build:lib && npm run build:theme && npm run start",
    "build": "rimraf ./dist && npm run build:lib && npm run build:theme && npm run build:app",
    "build:app": "ng build --prod",
    "build:lib": "ng build ngx-present && npm run build:lib-styles && npm run build:lib-assets && npm run build:readme",
    "build:lib-styles": "cpx \"./projects/w11k/ngx-present/src/lib/style/**/*\" \"./dist/w11k/ngx-present/style\"",
    "build:lib-assets": "cpx \"./projects/w11k/ngx-present/src/assets/**/*\" \"./dist/w11k/ngx-present/assets\"",
    "build:readme": "cpx \"./readme.md\" \"./dist/w11k/ngx-present\"",
    "build:theme": "ng build ngx-present-theme && npm run build:theme-styles && npm run build:theme-assets",
    "build:theme-styles": "cpx \"./projects/thecodecampus/ngx-present-theme/src/lib/style/**/*\" \"./dist/thecodecampus/ngx-present-theme/style\"",
    "build:theme-assets": "cpx \"./projects/thecodecampus/ngx-present-theme/src/assets/**/*\" \"./dist/thecodecampus/ngx-present-theme/assets\"",
    "build:stats": "ng build --prod --stats-json",
    "publish:lib": "npm publish dist/w11k/ngx-present",
    "publish:theme": "npm publish dist/thecodecampus/ngx-present-theme",
    "publish": "npm run publish:lib && npm run publish:theme",
    "bundle-report": "webpack-bundle-analyzer dist/demo-app/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@thecodecampus/ngx-present-theme": "./projects/thecodecampus/ngx-present-theme",
    "@w11k/ngx-present": "./projects/w11k/ngx-present",
    "core-js": "^2.5.4",
    "rimraf": "2.6.2",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cpx": "1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-mocks": "6.1.0",
    "ng-packagr": "^3.0.0",
    "protractor": "~5.3.0",
    "scss-bundle": "2.3.2",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2",
    "webpack-bundle-analyzer": "2.13.1"
  }
}
